	<subDataset name="penjualan">
		<queryString language="SQL">
			<![CDATA[SELECT penjualan.id_transaksi, penjualan.no_faktur, penjualan.tanggal, barang.nama_barang, user.username, penjualan.jumlah, penjualan.sub_total
FROM penjualan penjualan
INNER JOIN barang barang ON penjualan.kd_barang = barang.kd_barang
INNER JOIN user user ON penjualan.id_user = user.id_user
]]>
